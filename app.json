{
  "expo": {
    "name": "ETOH Tracker",
    "slug": "etoh-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#3498db"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.etohtracker.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#3498db"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.etohtracker.app"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "name": "ETOH Tracker",
      "shortName": "ETOH",
      "lang": "en",
      "scope": "/",
      "themeColor": "#4f46e5",
      "backgroundColor": "#ffffff",
      "display": "standalone",
      "orientation": "portrait",
      "startUrl": "/",
      "preferRelatedApplications": false,
      "bundler": "metro",
      "manifest": "./public/manifest.json",
      "serviceWorker": {
        "src": "./public/sw.js",
        "scope": "/"
      }
    },
    "extra": {
      "eas": {
        "projectId": "etoh-tracker"
      }
    }
  }
}
